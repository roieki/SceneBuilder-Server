{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "apollo-boost": "^0.1.12",
    "bcryptjs": "2.4.3",
    "chalk": "^2.4.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-yoga": "1.15.1",
    "jsonwebtoken": "8.3.0",
    "pluralize": "^7.0.0",
    "prisma-binding": "2.1.3"
  },
  "devDependencies": {
    "dotenv": "6.0.0",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "prisma": "1.13.4"
  }
}
